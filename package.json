{
  "name": "camilo-autocomplete-toggle",
  "displayName": "SilentCoder - Autocomplete Toggle",
  "description": "Toggle autocomplete on/off with timer - Take control of your coding flow",
  "version": "0.0.2",
  "publisher": "CamiloCharry",
  "author": {
    "name": "Camilo"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Chinyiskan/camilo-autocomplete-toggle"
  },
  "engines": {
    "vscode": "^1.100.0"
  },
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "keywords": [
    "autocomplete",
    "productivity",
    "toggle",
    "timer",
    "coding flow"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autocomplete-toggle.toggle",
        "title": "Toggle Autocomplete"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
